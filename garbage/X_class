@dataclass
class X_class:
    all_X_class: ClassVar[List['X_class']] = []
    X_class_name:str
    #1
    human:str
    animal:str
    inanimate:str
    #2
    paid_item:str
    food:str
    vehicle:str
    #3
    building:str
    job:str
    merchant:str
    #4
    carry_around:str
    rent        :str
    second_hand :str
    #5
    present:str
    container:str
    insert:str
    #6
    child_use:str
    seats:str
    hours:str
    #woman
    X_woman_A:list
    X_woman_B:list
    woman_X_A:list
    woman_X_B:list
    #animal
    X_animal_A:list
    X_animal_B:list
    animal_X_A:list
    animal_X_B:list
    #robber
    X_robber_A:list
    X_robber_B:list
    robber_X_A:list
    robber_X_B:list
    #roller_coaster
    X_roller_coaster_A:list
    X_roller_coaster_B:list
    roller_coaster_X_A:list
    roller_coaster_X_B:list


    def __post_init__(self):
        X_class.all_X_class.append(self)
        #1
        if self.X_class_name:
            self.X_robber_A.extend([list_robber_info, list_robber_fight, list_robber_lose])
        if self.human:
            self.X_robber_A.extend([list_human_robber_A]) # "I asked about recommended tourist spots."
            self.robber_X_A.extend([list_human_robber_B]) #

        if self.animal:
            self.X_woman_A.extend([list_animal_A]) # "He poops everywhere."
            self.woman_X_B.extend([list_animal_B]) # "Let me pet this next time."
            self.woman_X_A.extend([list_woman_animal_A])

        if self.paid_item:
            self.X_woman_A.extend([list_paid_item_A]) # "I bought it cheaply."
            self.woman_X_B.extend([list_paid_item_B]) # "How much did it cost?"
            self.X_roller_coaster_A.extend([list_paid_item_A, list_cheap_A])
            self.roller_coaster_X_B.extend([list_paid_item_B, list_cheap_B])
            self.woman_X_A.extend([list_woman_paid_item_A])

        #2
        if self.inanimate:
            pass
        if self.food:
            self.X_woman_A.extend([list_food_A]) # "It was delicious."
            self.woman_X_B.extend([list_food_B]) # "I've eaten this before."
        if self.vehicle:
            self.X_woman_A.extend([list_vehicle_A]) # "I like riding it."
            self.woman_X_B.extend([list_vehicle_B]) # "I like riding it."
        #3
        if self.building:
            self.X_roller_coaster_A.extend([list_building_A])
            self.roller_coaster_X_B.extend([list_building_B])
            self.X_woman_A.extend([list_building_woman_A])
            self.woman_X_A.extend([list_building_woman_B])
        if self.job:
            self.X_robber_A.extend([list_job_A]) # "I used to do things like them too."
            self.robber_X_B.extend([list_job_B]) #
        if self.merchant:
            self.X_woman_A.extend([list_merchant_A]) # "They offer coupons"
            self.woman_X_B.extend([list_merchant_B]) #
        #4
        if self.carry_around:
            self.X_woman_A.extend([list_carry_around_A]) # "I left it at a friend's house."
            self.woman_X_B.extend([list_carry_around_B]) #
        if self.rent:
            self.X_woman_A.extend([list_rent_A]) # "Wanna borrow?"
            self.woman_X_B.extend([list_rent_B]) # "Please lend it to me."
        if self.second_hand:
            self.X_woman_A.extend([list_second_hand_A]) # "This is second hand."
            self.woman_X_B.extend([list_second_hand_B]) # "Is this second hand?"
        #5
        if self.present:
            self.X_woman_A.extend([list_present_A]) # "I received this as a gift from my mother."
            self.woman_X_B.extend([list_present_B]) # "Did your mother give it to you as a gift?"
        if self.container:
            self.X_woman_A.extend([list_container_A])
            self.woman_X_B.extend([list_container_B])
        if self.insert:
            self.X_woman_A.extend([list_insert_A])
            self.woman_X_B.extend([list_insert_B])
        #6
        if self.child_use:
            self.X_woman_A.extend([list_child_use_A]) # "I've been using this since I was ten years old."
            self.woman_X_B.extend([list_child_use_B])
        if self.seats:
            self.X_roller_coaster_A.extend([list_seats_A]) # "There was a noisy child, so I scolded him and made him quiet."
            self.roller_coaster_X_B.extend([list_seats_B])
        if self.hours:
            self.X_roller_coaster_A.extend([list_hours_A]) # "I spent a few hours there."
            self.roller_coaster_X_B.extend([list_hours_B])


